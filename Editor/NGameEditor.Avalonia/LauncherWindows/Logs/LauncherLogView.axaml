<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:logs="clr-namespace:NGameEditor.ViewModels.LauncherWindows.Logs;assembly=NGameEditor.ViewModels"
             xmlns:example="clr-namespace:NGameEditor.Avalonia.LauncherWindows.Logs"
             x:Class="NGameEditor.Avalonia.LauncherWindows.Logs.LauncherLogView"
             x:DataType="logs:LauncherLogViewModel"
             d:DataContext="{x:Static example:LauncherLogDesignData.LauncherLogExample}"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450">

	<StackPanel>
		<ListBox ItemsSource="{Binding LogEntries}" />
	</StackPanel>

</UserControl>
