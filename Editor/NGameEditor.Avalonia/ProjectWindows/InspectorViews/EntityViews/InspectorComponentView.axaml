<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:inspectorViews="clr-namespace:NGameEditor.ViewModels.ProjectWindows.InspectorViews;assembly=NGameEditor.ViewModels"
             xmlns:components="clr-namespace:NGameEditor.ViewModels.ProjectWindows.InspectorViews.Components;assembly=NGameEditor.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="components:InspectorComponentViewModel"
             x:Class="NGameEditor.Avalonia.ProjectWindows.InspectorViews.EntityViews.InspectorComponentView">
	<StackPanel>
		<Label Content="{Binding  ComponentState.Name}" />
		<ListBox ItemsSource="{Binding Properties}" />
	</StackPanel>
</UserControl>
