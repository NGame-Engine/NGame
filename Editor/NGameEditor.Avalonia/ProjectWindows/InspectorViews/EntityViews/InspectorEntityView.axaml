<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:inspectorViews="clr-namespace:NGameEditor.ViewModels.ProjectWindows.InspectorViews;assembly=NGameEditor.ViewModels"
             xmlns:example="clr-namespace:NGameEditor.Avalonia.ProjectWindows.InspectorViews.EntityViews"
             x:Class="NGameEditor.Avalonia.ProjectWindows.InspectorViews.EntityViews.InspectorEntityView"
             x:DataType="inspectorViews:InspectorEntityViewModel"
             d:DataContext="{x:Static example:EntityInspectorDesignData.Example}"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450">

	<StackPanel>
		<TextBox Text="{Binding EntityName}" IsEnabled="{Binding CanEditName}" />
		<ListBox ItemsSource="{Binding ComponentEditors}" />
	</StackPanel>

</UserControl>
