<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:inspectorViews="clr-namespace:NGameEditor.ViewModels.ProjectWindows.InspectorViews;assembly=NGameEditor.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="inspectorViews:PropertyViewModel"
             x:Class="NGameEditor.Avalonia.ProjectWindows.InspectorViews.EntityViews.PropertyView">

	<StackPanel Orientation="Horizontal">
		<Label Content="{Binding Name}" />
		<ContentControl Content="{Binding EditorViewModel}"/>
	</StackPanel>

</UserControl>
