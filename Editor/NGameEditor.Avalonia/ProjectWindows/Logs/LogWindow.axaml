<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:logs="clr-namespace:NGameEditor.ViewModels.ProjectWindows.Logs;assembly=NGameEditor.ViewModels"
        xmlns:example="clr-namespace:NGameEditor.Avalonia.ProjectWindows.Logs"
        Title="LogWindow"
        x:Class="NGameEditor.Avalonia.ProjectWindows.Logs.LogWindow"
        x:DataType="logs:LogWindowModel"
        d:DataContext="{x:Static example:ProjectLogDesignData.Example}"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450">

	<ListBox ItemsSource="{Binding LogEntries}" />

</Window>
